{
  "name": "angular2-todo-tutorial",
  "version": "1.0.0",
  "description": "angular 2 Todo application sample",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "http-server": "http-server",
    "test": "karma start karma.conf.js",
    "coverage": "node_modules/.bin/remap-istanbul -i report/coverage/coverage-final.json -o report/coverage/ -t html",
    "start": "concurrently -r \"npm run tsc:w\" \"npm run lite\" ",
    "start-heroku": "concurrently \"npm run tsc\" \"npm run http-server\" ",
    "postinstall": "typings install"
  },
